module encontrarPareja
	(
	input reg  [4:0] MatrizJuego [0:3][0:3]
	
	input [4:0] primeraX
	input [4:0] primeraY

	input [4:0] segundaX
	input [4:0] segundaY

	output banderaPareja
	
	);
	
	always @ (posedge posX or posedge posY) begin
	
	if(MatrizJuego[primeraX][primeraY] == MatrizJuego[primeraX][primeraY]);
		assign banderaPareja = 1;
	elif
		assign banderaPareja = 0;

	
	
	end
endmodule

	
	